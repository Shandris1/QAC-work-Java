import java.awt.*;
public class Window {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Frame Win=new Frame();
TextField Ti=new TextField();
Button Bi= new Button("click");
Win.add(Ti,BorderLayout.NORTH);
Win.add(Bi,BorderLayout.CENTER);
Win.setSize(300,500);
Win.setVisible(true);
	}

}
