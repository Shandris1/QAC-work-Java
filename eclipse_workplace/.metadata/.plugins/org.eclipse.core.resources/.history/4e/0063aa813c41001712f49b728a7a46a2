
public class Division {

	public static void main(String[] xyz) {
		// TODO Auto-generated method stub
int A,B,C;
A=B=C=0;
try{
	A=Integer.parseInt(xyz[0]);
	B=Integer.parseInt(xyz[1]);
	C=A/B;
	System.out.println("The result ="+C);
}
catch(NumberFormatException x)
{System.out.println("Please only numeric values");
}
catch(ArithmeticException ABC)
{
	System.out.println("Cannot divide anything by 0");
	}
catch (ArrayIndexOutOfBoundsException AS)
{
	System.out.println("Please provide 2 values");
	}
catch(Exception X)
	{
System.out.print("dsajio");		
	}

}}
